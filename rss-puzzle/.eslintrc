{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "extends": ["airbnb-typescript/base", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.json",
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "no-console": "warn",
  },
}
