{
  "name": "rss-puzzle",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "webpack serve",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build-dev": "webpack",
    "build-prod": "webpack --node-env=production",
    "clear": "rd /s /q dist",
    "format": "prettier src --write",
    "ci:format": "prettier src --check",
    "prepare": "cd .. && husky rss-puzzle/.husky",
    "validate-branch-name": "npx validate-branch-name"
  },
  "author": "kagerka",
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.0",
    "prettier": "3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate-branch-name"
    }
  },
  "validate-branch-name": {
    "pattern": "^(init|chore|feat|fix|docs|refactor)/RSS-PZ-[0-9][0-9]_[a-zA-Z]+",
    "errorMsg": "Branch name is not valid."
  }
}
